{
  "compilerOptions": {
    "target": "ES5",
    "baseUrl": ".",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "experimentalDecorators": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "#apis/*": ["./resources/js/apis/*"],
      "#app/*": ["./resources/js/app/*"],
      "#aws/*": ["./resources/js/aws/*"],
      "#assets/*": ["./resources/js/assets/*"],
      "#commons/*": ["./resources/js/commons/*"],
      "#components/*": ["./resources/js/components/*"],
      "#contexts/*": ["./resources/js/contexts/*"],
      "#hooks/*": ["./resources/js/hooks/*"],
      "#mocks/*": ["./resources/js/mocks/*"],
      "#layouts/*": ["./resources/js/layouts/*"],
      "#libs/*": ["./resources/js/libs/*"],
      "#pages/*": ["./resources/js/pages/*"],
      "#prisma/*": ["./resources/js/prisma/*"],
      "#public/*": ["./resources/js/public/*"],
      "#stores/*": ["./resources/js/stores/*"],
      "#queries/*": ["./resources/js/queries/*"],
      "#styles/*": ["./resources/js/styles/*"],
      "#types/*": ["./resources/js/types/*"],
      "#ui/*": ["./resources/js/ui/*"],
      "#utils/*": ["./resources/js/utils/*"],
      "ziggy-js": ["./vendor/tightenco/ziggy"]
    }
  },
  "include": [
    "resources/js/**/*.ts",
    "resources/js/**/*.tsx",
    "resources/js/**/*.d.ts"
  ],
  "exclude": ["node_modules"]
}
